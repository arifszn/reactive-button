n":[[0,12]]}}}],["size",{"_index":11,"t":{"29":{"position":[[0,4]]}}}],["state",{"_index":14,"t":{"33":{"position":[[9,5]]},"35":{"position":[[8,5]]}}}],["style",{"_index":12,"t":{"31":{"position":[[0,5]]}}}],["submit",{"_index":19,"t":{"39":{"position":[[5,6]]}}}],["support",{"_index":3,"t":{"11":{"position":[[0,7]]}}}],["tag",{"_index":21,"t":{"41":{"position":[[7,3]]}}}],["theme",{"_index":25,"t":{"47":{"position":[[7,5]]}}}],["us",{"_index":16,"t":{"37":{"position":[[0,5]]}}}],["usag",{"_index":9,"t":{"25":{"position":[[6,5]]}}}],["without",{"_index":15,"t":{"35":{"position":[[0,7]]}}}]],"pipeline":["stemmer"]}},{"documents":[{"i":3,"t":"React version >= 16.8 or above","s":"Requirements","u":"/reactive-button/docs/installation","h":"#requirements","p":1},{"i":4,"t":"Install via NPM npm install reactive-button Install via Yarn yarn add reactive-button","s":"Installation","u":"/reactive-button/docs/installation","h":"#installation","p":1},{"i":6,"t":"Reactive Button is a 3D animated react component to replace the traditional boring buttons. Change your button style without adding any UI framework. Comes with progress bar and the goal is to let the users visualize what is happening after a button click.","s":"Introduction","u":"/reactive-button/docs/introduction","h":"","p":5},{"i":8,"t":"Primary Secondary Teal Green Red Violet Blue Yellow Dark Light Primary Secondary Teal Green Red Loading Loading Loading Loading Loading","s":"Example","u":"/reactive-button/docs/introduction","h":"#example","p":5},{"i":10,"t":"3D Animated Supports icons Zero dependency Progress indicator Notification message Supports TypeScript Super easy to setup Extremely lightweight Super easy to customize And much more !","s":"Features","u":"/reactive-button/docs/introduction","h":"#features","p":5},{"i":12,"t":"You can show your support by starring this project.","s":"Support","u":"/reactive-button/docs/introduction","h":"#support","p":5},{"i":14,"t":"To contribute, see the contributing guide.","s":"Contribute","u":"/reactive-button/docs/introduction","h":"#contribute","p":5},{"i":16,"t":"MIT License","s":"License","u":"/reactive-button/docs/introduction","h":"#license","p":5},{"i":18,"t":"Playground for Reactive Button.","s":"Playground","u":"/reactive-button/docs/playground","h":"","p":17},{"i":20,"t":"Toggle or change the props to see changes. Click Me buttonState idleloadingsuccesserror idleText successText shadow outline animation className size tinysmallnormallarge messageDuration color primarySecondarydarklightgreenredyellowtealvioletblue loadingText errorText rounded block disabled width px height px Reset All","s":"Interactive Playground","u":"/reactive-button/docs/playground","h":"#interactive-playground","p":17},{"i":22,"t":"Edit code to see changes. Click Me function App() { const [state, setState] = useState('idle'); const onClickHandler = () => { setState('loading'); setTimeout(() => { setState('success'); }, 2000); }; return ( <ReactiveButton buttonState={state} onClick={onClickHandler} color={'primary'} idleText={'Click Me'} loadingText={ <> <FontAwesomeIcon icon={faCircleNotch} spin /> Loading </> } successText={ <> <FontAwesomeIcon icon={faThumbsUp} /> Success </> } errorText={ <> <FontAwesomeIcon icon={faBomb} /> Error </> } type={'button'} className={'class1 class2'} style={{ borderRadius: '5px', }} outline={false} shadow={false} rounded={false} size={'normal'} block={false} messageDuration={2000} disabled={false} buttonRef={null} width={null} height={null} animation={true} /> ); } function App() { const [state, setState] = useState('idle'); const onClickHandler = () => { setState('loading'); setTimeout(() => { setState('success'); }, 2000); }; return ( <ReactiveButton buttonState={state} onClick={onClickHandler} color={'primary'} idleText={'Click Me'} loadingText={ <> <FontAwesomeIcon icon={faCircleNotch} spin /> Loading </> } successText={ <> <FontAwesomeIcon icon={faThumbsUp} /> Success </> } errorText={ <> <FontAwesomeIcon icon={faBomb} /> Error </> } type={'button'} className={'class1 class2'} style={{ borderRadius: '5px', }} outline={false} shadow={false} rounded={false} size={'normal'} block={false} messageDuration={2000} disabled={false} buttonRef={null} width={null} height={null} animation={true} /> ); }","s":"Editor Playground","u":"/reactive-button/docs/playground","h":"#editor-playground","p":17},{"i":24,"t":"The targets of the below example: Show a button showing the text 'Submit'. After clicking the button, change the button text to 'Submitting' and send an HTTP request. Upon successful request, change the button text to 'Done' as success notification. import { useState } from 'react'; import ReactiveButton from 'reactive-button'; function App() { const [state, setState] = useState('idle'); const onClickHandler = () => { setState('loading'); // send an HTTP request setTimeout(() => { setState('success'); }, 2000); }; return ( <ReactiveButton buttonState={state} idleText=\"Submit\" loadingText=\"Submitting\" successText=\"Done\" onClick={onClickHandler} /> ); } export default App;","s":"Usage","u":"/reactive-button/docs/usage","h":"","p":23},{"i":26,"t":"Reactive Button has all the functionalities of a normal button.","s":"Other Usage","u":"/reactive-button/docs/usage","h":"#other-usage","p":23},{"i":28,"t":"Reactive Button comes with 10 default color options. Primary Secondary Teal Green Red Violet Blue Yellow Dark Light return ( <> <ReactiveButton color=\"primary\" /> <ReactiveButton color=\"secondary\" /> <ReactiveButton color=\"teal\" /> <ReactiveButton color=\"green\" /> <ReactiveButton color=\"red\" /> <ReactiveButton color=\"violet\" /> <ReactiveButton color=\"blue\" /> <ReactiveButton color=\"yellow\" /> <ReactiveButton color=\"dark\" /> <ReactiveButton color=\"light\" /> </> );","s":"Color","u":"/reactive-button/docs/usage","h":"#color","p":23},{"i":30,"t":"There are 4 sizes available. Tiny Small Medium Large return ( <> <ReactiveButton size=\"tiny\" /> <ReactiveButton size=\"small\" /> <ReactiveButton size=\"medium\" /> <ReactiveButton size=\"large\" /> </> );","s":"Size","u":"/reactive-button/docs/usage","h":"#size","p":23},{"i":32,"t":"Make the buttons more beautiful with these customization options. Outline Rounded Shadow return ( <> <ReactiveButton outline /> <ReactiveButton rounded /> <ReactiveButton shadow /> </> );","s":"Style","u":"/reactive-button/docs/usage","h":"#style","p":23},{"i":34,"t":"In your project, There might be existing state for loading indicator which accepts boolean value only. If you don't want to define new state for Reactive Button, then utilize the existing state. const [loading, setLoading] = useState(false); return ( <ReactiveButton buttonState={loading ? 'loading' : 'idle'} idleText={'Button'} loadingText={'Loading'} /> );","s":"Existing State","u":"/reactive-button/docs/usage","h":"#existing-state","p":23},{"i":36,"t":"You are not limited to use state always. return <ReactiveButton onClick={doSomething} idleText={'Some Text'} />;","s":"Without State","u":"/reactive-button/docs/usage","h":"#without-state","p":23},{"i":38,"t":"You can use your own icons. Don't forget to wrap them with a parent element. Send return ( <ReactiveButton idleText={ <span> <faReply /> Send </span> } /> );","s":"Using Icons","u":"/reactive-button/docs/usage","h":"#using-icons","p":23},{"i":40,"t":"If you need to submit form by button clicking, set the type prop as 'submit'. return ( <form> <input type=\"text\" name=\"username\" /> <input type=\"password\" name=\"password\" /> <ReactiveButton type={'submit'} idleText=\"Submit\" /> </form> );","s":"Form Submit","u":"/reactive-button/docs/usage","h":"#form-submit","p":23},{"i":42,"t":"To use Reactive button as anchor tag, simply wrap it with an anchor tag. Visit Github return ( <a href=\"https://github.com/\" target=\"_blank\"> <ReactiveButton idleText=\"Visit Github\" /> </a> ); Note:Playground","s":"Anchor Tag","u":"/reactive-button/docs/usage","h":"#anchor-tag","p":23},{"i":44,"t":"Props Type Description Default buttonState string 'idle' | 'loading' | 'success' | 'error' 'idle' onClick function Callback function when clicking button () => {} color string 'primary' | 'secondary' | 'dark' | 'light' | 'green' | 'red' | 'yellow' | 'teal' | 'violet' | 'blue' 'primary' idleText string | ReactNode Button text when idle 'Click Me' loadingText string | ReactNode Button text when loading 'Loading' successText string | ReactNode Button text when loading successful 'Success' errorText string | ReactNode Button text when loading failed 'Error' type string 'button' | 'submit' | 'reset' 'button' className string Button classnames '' style React.CSSProperties Custom style {} outline boolean Enable outline effect false shadow boolean Enable shadow effect false rounded boolean Enable rounded button false size string 'tiny' | 'small' | 'normal' | 'large' 'normal' block boolean Block Button false messageDuration number Success/Error message duration in millisecond 2000 disabled boolean Disable button false buttonRef React.Ref | null Button reference null width string | null Override button width null height string | null Override button height null animation boolean Button hover and click animation true","s":"Available Props","u":"/reactive-button/docs/usage","h":"#available-props","p":23},{"i":46,"t":"Reactive Button provides flexible styling option. Most of the common style changes can be achieved by using the style prop. <ReactiveButton style={{ borderRadius: '10px', color: 'black', }} /> <ReactiveButton style={{ borderRadius: '10px', color: 'black', }} /> Click Me However, you are free to create your own theme.","s":"Theming","u":"/reactive-button/docs/theming","h":"","p":45},{"i":48,"t":"Modify the values how you prefer and use it in your existing css file. .reactive-btn-wrapper, .reactive-btn { --reactive-button-min-width: 100px !important; --reactive-button-min-height: 35px !important; --reactive-button-font-size: 14px !important; --reactive-button-font-weight: 400 !important; --reactive-button-border-radius: 0px !important; --reactive-button-text-color: #ffffff !important; --reactive-progress-color: rgba(0, 0, 0, 0.15) !important; --reactive-button-primary-color: rgba(88, 103, 221, 1) !important; --reactive-button-secondary-color: rgba(108, 117, 125, 1) !important; --reactive-button-dark-color: rgba(66, 78, 106, 1) !important; --reactive-button-light-color: rgba(183, 186, 191, 1) !important; --reactive-button-green-color: rgba(37, 162, 51, 1) !important; --reactive-button-red-color: rgba(244, 81, 108, 1) !important; --reactive-button-yellow-color: rgba(255, 193, 7, 1) !important; --reactive-button-teal-color: rgba(0, 181, 173, 1) !important; --reactive-button-violet-color: rgba(100, 53, 201, 1) !important; --reactive-button-blue-color: rgba(66, 153, 225, 1) !important; --reactive-progress-outline-primary-color: rgba(88, 103, 221, 0.3) !important; --reactive-progress-outline-secondary-color: rgba(108, 117, 125, 0.3) !important; --reactive-progress-outline-dark-color: rgba(66, 78, 106, 0.3) !important; --reactive-progress-outline-light-color: rgba(214, 212, 212, 0.3) !important; --reactive-progress-outline-green-color: rgba(37, 162, 51, 0.3) !important; --reactive-progress-outline-red-color: rgba(244, 81, 108, 0.3) !important; --reactive-progress-outline-yellow-color: rgba(255, 193, 7, 0.3) !important; --reactive-progress-outline-teal-color: rgba(0, 181, 173, 0.3) !important; --reactive-progress-outline-violet-color: rgba(100, 53, 201, 0.3) !important; --reactive-progress-outline-blue-color: rgba(66, 153, 225, 0.3) !important; }","s":"Custom Theme","u":"/reactive-button/docs/theming","h":"#custom-theme","p":45}],"index":{"version":"2.3.9","fields":["t"],"fieldVectors":[["t/3",[0,3.079,1,4.405,2,0.749,3,4.405,4,4.405]],["t/4",[5,5.167,6,4.772,7,4.772,8,1.472,9,1.045,10,4.772,11,3.883]],["t/6",[0,2.343,8,1.034,9,1.119,12,2.744,13,2.044,14,3.353,15,3.353,16,3.353,17,3.353,18,1.805,19,2.044,20,3.353,21,3.353,22,3.35